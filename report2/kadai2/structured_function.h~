#ifndef _FUNCTION_STRUCTURING_H_
#define _FUNCTION_STRUCTURING_H_

#define VALUE_LEN 10
    
typedef struct Node_tag
{
    char value[VALUE_LEN];
    struct Node_tag *left;
    struct Node_tag *right;
} Node;

void parse(Node *);
Node *create_tree(int *, char **);
double calculate(Node *);

#endif

